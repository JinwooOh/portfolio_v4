{
  "name": "porfolio_project",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^1.3.0",
    "ajv": "^6.5.1",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "express-handlebars": "^3.0.0",
    "material-ui": "^0.20.1",
    "nodemailer": "^4.6.7",
    "prop-types": "^15.6.2",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4"
  },
  "scripts": {
    "build": "react-scripts build",
    "server": "nodemon server.js",
    "eject": "react-scripts eject",
    "build-css": "node-sass-chokidar src/css -o src/css",
    "watch-css": "npm run build-css && node-sass-chokidar src/css -o src/css --watch --recursive",
    "devserver": "react-scripts start",
    "dev": "npm-run-all --parallel watch-css devserver",
    "start": "forever start --minUptime 1000 --spinSleepTime 1000 server.js",
    "stop": "forever stop server.js"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "node-sass-chokidar": "^1.3.0",
    "nodemon": "^1.17.4",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.13.5",
    "forever": "^0.15.3"
  },
  "homepage": "http://jinwoo-oh.com",
  "proxy": "http://localhost:5000"
}
